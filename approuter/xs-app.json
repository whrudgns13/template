{
    "welcomeFile": "/webapp/index.html",
    "login" : {
        "callbackEndpoint": "/login/callback"
    },
    "logout" : {
        "logoutEndpoint": "/logout"
    },
    "routes": [
        {
            "source": "^/app",
            "target": "/",
            "destination": "server"
        },
        {
            "source": "^/webapp",
            "target": "/",
            "destination": "ui5app",
            "scope": "$XSAPPNAME.User"
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        }
    ]
}