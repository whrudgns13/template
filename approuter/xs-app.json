{
    "welcomeFile": "/webapp/index.html",
    "logout" : {
        "logoutEndpoint": "/logout"
    },
    "routes": [
        {
            "source": "^/app",
            "target": "/",
            "destination": "server"
        },
        {
            "source": "^/webapp",
            "target": "/",
            "destination": "ui5app",
            "scope": "$XSAPPNAME.User"
        },
        {
            "source": "^/user-api(.*)",
            "target": "$1",
            "service": "sap-approuter-userapi"
        }
    ],
    "errorPage": [
        {"status": [400,401,402,403], "file": "./error.html"}
    ]
}